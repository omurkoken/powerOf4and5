import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        int n;

        Scanner input= new Scanner(System.in);
        System.out.print("kuvvetini almak istediğiniz sınır nedir?: ");
        n=input.nextInt();

        for (int i=1 ; i<=n; i*=4)
            System.out.println("Girilen sınırda 4ün kuvvetleri:"+i);
        for (int i=1; i<=n; i*=5)
        System.out.println("Girilen sınırda 5in kuvvetleri:"+i);

    }
}
